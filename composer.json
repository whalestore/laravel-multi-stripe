{
  "name": "whalestore/laravel-multi-stripe",
  "description": "Multi Stripe account (test/live) support for Laravel Cashier (Stripe) without modifying Cashier itself.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0",
    "laravel/cashier": "^15.0|^16.0",
    "stripe/stripe-php": "^13.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Whalestore\\LaravelMultiStripe\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Whalestore\\LaravelMultiStripe\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Whalestore\\LaravelMultiStripe\\Providers\\MultiStripeServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
